{
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".scss", ".js", ".jsx"],
        "alias": {
          "@components": "./src/components",
          "@pages": "./src/pages",
          "@utils": "./src/utils",
          "@appIcons": "./src/assets/icons",
          "@appImages": "./src/assets/images",
          "@appSounds": "./src/assets/sounds",
          "@appVideos": "./src/assets/videos",
          "@appReduxStore": "./src/app-redux-store"
        }
      }
    ]
  ]
}
